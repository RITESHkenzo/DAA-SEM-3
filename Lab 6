#include <stdio.h>
#define MAX 10
int main() {
int n, i, j, k;
int A[MAX][MAX], B[MAX][MAX], C[MAX][MAX];
printf("Enter order ofsquare matrix (n x n): ");
scanf("%d", &n);
// Input matrix A
printf("Enter elements of Matrix A:\n");
for(i = 0; i < n; i++) {
for(j = 0; j < n; j++) {
scanf("%d", &A[i][j]);
}
}
// Input matrix B
printf("Enter elements of Matrix B:\n");
for(i = 0; i < n; i++) {
for(j = 0; j < n; j++) {
scanf("%d", &B[i][j]);
}
}
// Initialize result matrix
for(i = 0; i < n; i++) {
for(j = 0; j < n; j++) {
C[i][j] = 0;
}
}
// Matrix multiplication
for(i = 0; i < n; i++) {
for(j = 0; j < n; j++) {
for(k = 0; k < n; k++) {
C[i][j] += A[i][k] * B[k][j];
}
}
}
// Print result
printf("Resultant Matrix C (A x B):\n");
for(i = 0; i < n; i++) {
for(j = 0; j < n; j++) {
printf("%d ", C[i][j]);
}
printf("\n");
}
return 0;
}
